{
  "name": "@artemis/artemis-ui",
  "version": "0.0.1",
  "description": "Artemis UI",
  "type": "module",
  "main": "dist/artemis-ui.umd.js",
  "module": "dist/artemis-ui.es.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Artemis-pets/Artemis-components.git"
  },
  "keywords": [
    "lit",
    "typescript",
    "javascript",
    "node",
    "tailwind"
  ],
  "author": {
    "name": "Gerardo Heredia",
    "email": "gerardo.heredia@proton.me"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:cem": "custom-elements-manifest analyze --globs 'src/components/**/*.ts' --litelement --outdir . --exclude src/components/index.ts",
    "preview": "vite preview",
    "prettier:format": "prettier --write \"**/*.{ts,html,css,json,md}\"",
    "prettier:check": "prettier --check \"**/*.{ts,html,css,json,md}\"",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "lint": "eslint --config eslint.config.ts .",
    "lint:fix": "eslint --config eslint.config.ts . --fix",
    "check:all": "npm run prettier:check && npm run lint"
  },
  "dependencies": {
    "lit": "^3.3.0",
    "material-symbols": "^0.31.9",
    "storybook": "^9.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@eslint/config-inspector": "^1.2.0",
    "@eslint/css": "^0.11.0",
    "@eslint/js": "^9.35.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@html-eslint/eslint-plugin": "^0.46.2",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/web-components-vite": "^9.1.1",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.3.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "playwright": "^1.54.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "custom-elements-manifest.json"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/artemis-ui.es.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/artemis-ui.umd.js"
      }
    }
  },
  "license": "MIT"
}
